{
	"parser": "@babel/eslint-parser",
	"env": {
		"commonjs": true,
		"es2021": true,
		"node": true
	},
	"extends": ["standard", "prettier"],
	"plugins": ["prettier"],
	"parserOptions": {
		"ecmaVersion": 12
	},
	"rules": {
		"prettier/prettier": "error",
		"indent": ["error", "tab"],
		"no-useless-return": "off"
	}
}
